{
	"name": "Cite-D-Lite",
	"version": "1.0",
	"description": "Ma'ayan Lab",
	
	"icons":{
		"128": "icon_128.png"
	},
	
	"browser_action": {
		"default_icon": "icon_128.png"
	},
	
	"permissions": [
		"downloads"
	],
	
	"content_scripts": [
		{
			"matches": [
				"http://www.ncbi.nlm.nih.gov/*"
			],
			"css": [
				"button.css"
			],
			"js": [
				"underscore-min.js", "jquery.js", "summarizer.js", "out.js"
			]
		}
	],
	
	"web_accessible_resources": [
		"icon_128.png"
	],

	"manifest_version": 2
}